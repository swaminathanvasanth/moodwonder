+		seekBarBOne.setProgress(oneB_Value);
+		//seekBarTopOne.setEnabled(false);
+		
+		seekBarBTwo = (SeekBar) findViewById(R.id.seekBarBtwo);
+		twoB_Value = (int) (Float.parseFloat(two_rating_Bottom) * 10);
+		seekBarBTwo.setProgress(twoB_Value);
